[autoload]

Scoreboard="*res://src/Scoreboard.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/extendable_inspector/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
environment/defaults/default_clear_color=Color(0.0313726, 0.215686, 0.25098, 1)
